<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.GameShop.dao.UpdateUserImgDao">
    
    <update id="updateUserPhoto">
        UPDATE USER
        <set>
                photo = #{photo}
        </set>
        <where>
            id = #{id}
        </where>
    </update>
</mapper>